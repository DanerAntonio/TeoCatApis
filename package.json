{
  "name": "teocat-api",
  "version": "1.0.0",
  "description": "API SOA para TeoCat",
  "main": "src/index.js",
  "type": "module",

  "engines": {
    "node": ">=18.x",
    "npm": ">=9.x"
  },
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "npm test -- --watch",
    "test:coverage": "npm test -- --coverage"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "bcryptjs": "^3.0.2",
    "cloudinary": "^2.6.0",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "express-validator": "^7.2.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "moment-timezone": "^0.6.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.2",
    "mysql2": "^3.14.0",
    "nodemailer": "^6.10.1",
    "socket.io": "^4.8.1",
    "streamifier": "^0.1.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "eslint": "^9.24.0",
    "jest": "^29.7.0",
    "node-mocks-http": "^1.17.2",
    "nodemon": "^3.1.9",
    "supertest": "^7.1.1"
  },
  "jest": {
  "testEnvironment": "node",
  "transform": {},
  "globals": {
    "jest": {
      "useESM": true
    }
  },
  "testMatch": [
  "**/tests/**/*.test.js"
],
  "collectCoverage": true,
  "coverageDirectory": "coverage",
  "coverageReporters": [
    "text",
    "lcov",
    "html"
  ]
},
  "keywords": [
    "api",
    "soa",
    "express",
    "teocat"
  ]
}
